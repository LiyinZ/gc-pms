<!doctype html>
<html lang="en">
<head>
    {% block head %}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}{% endblock %} | Barrie Student Rental</title>
    <link rel="stylesheet" href="{{ cssPath }}style.css" media="screen,projection"/>
    {% endblock %}
</head>

<body>
<div class="wrapper-for-content-outside-of-footer">
{% block header %}
    {% include 'partials/header.twig' %}
{% endblock %}

{% block content %}{% endblock %}
</div>

{% block footer %}
    {% include 'partials/footer.twig' %}
{% endblock %}
</body>
</html>